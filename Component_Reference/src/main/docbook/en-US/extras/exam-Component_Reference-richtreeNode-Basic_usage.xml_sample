<rich:tree nodeType="#{node.type}" var="node"
           value="#{treeBean.rootNodes}">
   <rich:treeNode type="country">
      #{node.name}
   </rich:treeNode>
   <rich:treeNode type="state">
      #{node.name}
   </rich:treeNode>
   <rich:treeNode type="city">
      #{node.name}
   </rich:treeNode>
</rich:tree>
